{
  "name": "@externs/preact",
  "version": "1.2.0",
  "description": "The Externs For Preact.",
  "main": "build/index.js",
  "module": "src/index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec test/mask",
    "spec": "yarn t test/spec",
    "mask": "yarn t test/mask",
    "test-build": "ALAMODE_ENV=test-build yarn test",
    "wiki": "NODE_DEBUG=doc doc wiki -W ../preact.wiki -n preact",
    "lint": "eslint .",
    "doc": "NODE_DEBUG=doc doc -o README.md",
    "b": "alamode src -o build -s",
    "d": "yarn-s d1 externs",
    "d1": "typal build/index.js",
    "externs": "typal types/externs.js -e",
    "build": "yarn-s d b doc",
    "e": "alanode"
  },
  "files": [
    "build",
    "src",
    "types"
  ],
  "externs": "types/externs.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/dpck/preact.git"
  },
  "keywords": [
    "preact",
    "externs"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dpck/preact/issues"
  },
  "homepage": "https://github.com/dpck/preact#readme",
  "devDependencies": {
    "alamode": "^2.4.0",
    "documentary": "^1.32.1",
    "eslint-config-artdeco": "1.0.1",
    "typal": "^1.22.0",
    "yarn-s": "1.1.0",
    "zoroaster": "^4.1.1-alpha"
  }
}
